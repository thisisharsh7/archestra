{
  "name": "experiments",
  "version": "0.0.1",
  "description": "Record all your interactions with Claude Code as you develop your projects",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
    "proxy:dev": "nodemon --exec \"tsc && node dist/main.js\" --watch src --ext ts",
    "cli-chat-with-guardrails": "tsx src/cli-chat.ts",
    "lint": "biome check",
    "lint:fix": "biome check --write",
    "lint:fix:unsafe": "biome check --write --unsafe",
    "type-check": "tsc --noEmit"
  },
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "keywords": [
    "archestra",
    "nodejs",
    "interceptor"
  ],
  "author": "Archestra",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "nodemon": "^3.1.10",
    "pino-pretty": "^13.1.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.38.0",
    "@aws-sdk/client-bedrock-runtime": "^3.936.0",
    "@google/genai": "^1.24.0",
    "dotenv": "^17.2.2",
    "fastify": "^5.6.1",
    "openai": "^5.23.0"
  },
  "pnpm": {
    "overrides": {
      "jws": "4.0.1"
    }
  }
}
